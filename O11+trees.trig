@prefix K: <http://example.org/K#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

K:K {
    K:K a rdfs:Class .

    <http://example.org/K#instance_0:undefined> xsd:string K:S .

    <http://example.org/K#instance_1:undefined> xsd:string K:S .

    K:Aa rdfs:member K:A .

    K:Ab rdfs:member K:A .

    K:B rdfs:isDefinedBy K:Aa,
            K:Ab .

    K:A rdfs:isDefinedBy K:K .

    K:S rdf:value K:B .
}

