@prefix A: <http://example.org/A#> .
@prefix B: <http://example.org/B#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

A:A {
    A:A a rdfs:Class .

    A:L rdfs:member A:A .
}

B:B {
    B:B a rdfs:Class .

    B: xsd:boolean B:Log .

    B:Log rdf:value B:B .
}

